command[check_disk_root]=@LIBDIR@/nagios/plugins/check_disk -w 25 -c 5 -p /
command[check_disk_logs]=@LIBDIR@/nagios/plugins/check_disk -w 25 -c 5 -p /var/log
command[check_disk_backup]=@LIBDIR@/nagios/plugins/check_disk -w 25 -c 5 -p /var/backup
command[check_disk_var]=@LIBDIR@/nagios/plugins/check_disk -w 25 -c 5 -p /var
command[check_total_procs]=@LIBDIR@/nagios/plugins/check_procs -w 150 -c 200
command[check_apt]=@LIBDIR@/nagios/plugins/check_apt
command[check_ntp]=@LIBDIR@/nagios/plugins/check_ntp -H pool.ntp.org
command[check_mysql]=@LIBDIR@/nagios/plugins/check_mysql -u root -p rootpassword
command[check_swap]=@LIBDIR@/nagios/plugins/check_swap -w 99% -c 50%
command[check_load]=@LIBDIR@/nagios/plugins/check_load -w 6,5,4 -c 7,6,5
command[check_procs]=@LIBDIR@/nagios/plugins/check_procs -w 150 -c 200
command[check_cpu]=@LIBDIR@/nagios/plugins/check_cpu 25 40
command[check_ipmi]=/usr/bin/sudo @LIBDIR@/nagios/plugins/check_ipmi
command[check_raid]=@LIBDIR@/nagios/plugins/check_raid
command[check_cpu_args]=@LIBDIR@/nagios/plugins/check_cpu $ARG1$ $ARG2$
